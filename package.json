{
  "name": "nestjs-auth0-monorepo",
  "private": true,
  "scripts": {
    "build": "turbo build --filter=@jimmycamus/nestjs-auth0",
    "dev": "pnpm i && pnpm build &&turbo run dev",
    "test": "turbo run test --filter=@jimmycamus/nestjs-auth0",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
