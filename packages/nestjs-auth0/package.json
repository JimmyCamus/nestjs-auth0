{
  "name": "@jimmycamus/nestjs-auth0",
  "license": "MIT",
  "version": "1.0.1",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "description": "Auth0 integration for NestJS applications.",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JimmyCamus/nestjs-auth0/tree/main"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "build": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "@nestjs/axios": "4.0.1",
    "@nestjs/common": "11.0.1",
    "@nestjs/core": "11.0.1",
    "axios": "1.11.0",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@types/jest": "29.5.4",
    "@types/node": "20.5.9",
    "@types/supertest": "2.0.12",
    "eslint": "8.57.0",
    "jest": "29.6.4",
    "jest-mock": "29.6.3",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "@nestjs/common": ">=9.0.0",
    "@nestjs/core": ">=9.0.0",
    "rxjs": ">=7.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
